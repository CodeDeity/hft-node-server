{
  "name": "hft-node-server",
  "version": "1.0.0",
  "description": "server end code for hft team",
  "private": true,
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "egg": "^2.2.1",
    "egg-cors": "^2.1.0",
    "egg-redis": "^2.0.0",
    "egg-router-plus": "^1.2.1",
    "egg-scripts": "^2.5.0",
    "egg-security": "^2.2.3",
    "egg-sequelize": "^3.1.5",
    "egg-validate": "^1.0.0",
    "loader": "^2.1.1",
    "loader-koa": "^2.0.1",
    "markdown-it": "^8.4.2",
    "mysql2": "^1.5.3",
    "uuid": "^3.3.2",
    "validator": "^10.4.0"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.0.0",
    "egg-bin": "^4.3.5",
    "egg-ci": "^1.8.0",
    "egg-mock": "^3.17.3",
    "eslint": "^5.2.0",
    "eslint-config-egg": "^7.0.0",
    "webstorm-disable-index": "^1.2.0"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-hft-node-server",
    "stop": "egg-scripts stop --title=egg-server-hft-node-server",
    "restart": "npm run stop && npm run start",
    "docker": "egg-scripts start --title=egg-server-hft-node-server",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod",
    "migrate:new": "egg-sequelize migration:create",
    "migrate:up": "egg-sequelize db:migrate",
    "migrate:down": "egg-sequelize db:migrate:undo"
  },
  "ci": {
    "version": "8"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodeDeity/hft-node-server.git"
  },
  "author": "HFT Team",
  "license": "MIT"
}
